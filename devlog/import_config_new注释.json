      "30001": {
        //标签个性化.由于部分标签和通用规则不一样，因此引入该配置。不需要则不配置。
        //以 up_spell_30001_appear_start 为例
        "labeljoin": {
          //表示连接生成标签名的时候，在第1个位置插入 label 配置的字符
          "nameindex": 1,
          "label": "spell"
        },
        //标签个性化。标签的结束字符，和通用规则不一样的在这里配置。不需要则不配置。
        "startsuffix": "appear_start",
        "endsuffix": "appear_end",
        //firepoint 个性化。此配置中，有8方向，每个方向上的firepoint位置可能不一样，故单独配置。
